const jwt = require ('jsonwebtoken')
const requireAuth = (req, res, next) => {
const token = req.cookies.jwt
if (token) {
    jwt. verify(token, '388cd170b5793', (err, decodedToken) => {
        if(err){
            console.log(err)
        }else{
            res.json(decodedToken)
        }
    })
}else{
    res. redirect('/login')
}}
module.exports=requireAuth
